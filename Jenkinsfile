pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Hello World'
                sh 'python --version'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
